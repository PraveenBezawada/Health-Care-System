		<script>
			document.addEventListener("DOMContentLoaded", function() {
				var holidayCheckbox = document.getElementById("holidayCheckbox");
				var hoursInfoDiv = document.querySelector(".hours-info");
		
				// Initially hide the hours-info div if the checkbox is checked
