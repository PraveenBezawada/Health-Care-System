		<!-- start & end time validation -->
		<script>
			// Populate options for start time
			var startTimeSelect = document.getElementById("startTime");
			var endTimeSelect = document.getElementById("endTime");
		
			for (var i = 0; i < 24; i++) {
				var hour = (i < 10) ? "0" + i : i;
				startTimeSelect.options[startTimeSelect.options.length] = new Option(hour + ":00", hour + ":00");
			}
		
			// Function to update end time options based on selected start time
			function updateEndTimeOptions() {
				var selectedStartTime = document.getElementById("startTime").value;
		
				// Clear existing options
				endTimeSelect.innerHTML = "<option value='-'>-</option>";
		
				// Populate options for end time based on selected start time
				for (var i = parseInt(selectedStartTime.split(":")[0]) + 1; i <= 23; i++) {
					var hour = (i < 10) ? "0" + i : i;
					endTimeSelect.options[endTimeSelect.options.length] = new Option(hour + ":00", hour + ":00");
				}
			}
		
			// Event listener to update end time options when start time changes
			startTimeSelect.addEventListener("change", updateEndTimeOptions);
		
			// Form submission validation
			document.getElementById("timeForm").addEventListener("submit", function(event) {
				event.preventDefault();
				var startTime = document.getElementById("startTime").value;
				var endTime = document.getElementById("endTime").value;
		
				// Validate start time and end time selections
				if (startTime === "-" || endTime === "-") {
					alert("Please select both start time and end time.");
					return false;
				}
		
				// Convert start time and end time to minutes
				var startTimeMinutes = parseInt(startTime.split(':')[0]) * 60 + parseInt(startTime.split(':')[1]);
				var endTimeMinutes = parseInt(endTime.split(':')[0]) * 60 + parseInt(endTime.split(':')[1]);
		
				// Check if start time is less than end time
				if (startTimeMinutes >= endTimeMinutes) {
					alert("Start time must be less than end time.");
					return false;
				}
		
				// If validation passed, you can submit the form or perform other actions
				document.getElementById("timeForm").submit();
			});
		</script>
		
