<form id = "registerForm" onsubmit="return false"  >
    <div class="form-group form-focus">
        <input type="text" class="form-control floating" id = "firstName" required >
        <label class="focus-label">First Name</label>
    </div>
    <div class="form-group form-focus">
        <input type="text" class="form-control floating" id = "lastName" required>
        <label class="focus-label">Last Name</label>
    </div>
    <div class="form-group form-focus">
        <label for="Date-Of-Birth" class="floating-label">Date of Birth</label>
        <input type="date" id="DOB" class="form-control floating" required>
    </div>
	<br>
	<br>
    <div class="form-group form-focus">
        <label for="blood-group" class="floating-label">Blood Group</label>
        <select class="form-control floating" id = "bloodGrp">
            <option value="" disabled selected>Select Blood Group</option>
            <option value="A+">A+</option>
            <option value="A-">A-</option>
            <option value="B+">B+</option>
            <option value="B-">B-</option>
            <option value="AB+">AB+</option>
            <option value="AB-">AB-</option>
            <option value="O+">O+</option>
            <option value="O-">O-</option>
        </select>
    </div>
	<br>
	<br>
	<div class="form-group ">
		<input type="email" class="form-control floating" pattern="[a-zA-Z0-9._%+-]+@gmail\.com"
			title="Please enter a valid Gmail address" id="email" required>
		<label class="focus-label">Email Address</label><br>
		<button id="verifyEmail" class="btn btn-primary">Verify Email</button>
	</div>

    <!-- <div class="form-group ">
        <input type="email" class="form-control floating" pattern="[a-zA-Z0-9._%+-]+@gmail\.com" title="Please enter a valid Gmail address" id = "email" required>
        <label class="focus-label">Email Address</label><br>
		<button id = "verifyEmail">Verify Email</button>
    </div> -->
	<div class="form-group form-focus" >
		<input type="text" class="form-control floating hide" id="OTPinput">
		<label class="focus-label hide" id = "OTPLabel">Enter OTP</label>
		<button class="btn btn-primary hide" id="verifyOTP"  onclick="verify_OTP()">Verify OTP</button>
	</div>
    <div class="form-group form-focus">
        <input type="text" class="form-control floating" id = "contact" pattern="[0-9]{10}" title="Please enter a 10-digit mobile number" required>
        <label class="focus-label">Mobile Number</label>
    </div>
    <div class="form-group form-focus">
        <input type="text" class="form-control floating" id = "address" required>
        <label class="focus-label">Address</label>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6 form-focus">
            <input type="text" class="form-control floating" id = "city" required>
            <label class="focus-label">City</label>
        </div>
        <div class="form-group col-md-6 form-focus">
            <input type="text" class="form-control floating" id = "state" required>
            <label class="focus-label">State</label>
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6 form-focus">
            <input type="text" class="form-control floating" id = "pincode" required>
            <label class="focus-label">Pincode</label>
        </div>
    </div>
    <div class="form-group form-focus">
        <input type="password" class="form-control floating" id = "password"  pattern="^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)(?=.*[^\da-zA-Z]).{8,}$" title="Password must be at least 8 characters long and contain at least one uppercase letter, one lowercase letter, one number, and one special character" required>
        <label class="focus-label">Create Password</label>
    </div>
    <div class="form-group form-focus">
        <input type="password" class="form-control floating" id = "cnfrmPassword" required>
        <label class="focus-label">Confirm Password</label>
    </div>
    <button class="btn btn-primary btn-block btn-lg login-btn" id = "register_patient">Signup</button>
